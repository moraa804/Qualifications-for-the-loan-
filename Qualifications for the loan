#include<stdio.h>
int main ()
{
   int age;
   float income;
   
   printf("enter the age of the customer:");
   scanf("%d", &age);
   
   printf("enter the income:");
   scanf("%f", & income);
   
   age=21;
   income=21000;
   printf("congratulations! you qualify for the loan \n");
   
   return 0;
   }
   
